---
import Layout from '../layouts/Layout.astro';
import Footer from '../components/Footer.astro';
import BreadcrumbSchema from '../components/BreadcrumbSchema.astro';

const pageTitle = "Hizmetlerimiz - Profesyonel Çözümler | Tetik Otomotiv";
const pageDescription = "Hyundai, Kia, Toyota ve Nissan araçlarınız için motor, mekanik, şanzıman ve elektrik sistemlerinde garantili servis hizmeti.";

const breadcrumbItems = [
  { name: 'Anasayfa', url: '/' },
  { name: 'Hizmetlerimiz', url: '/hizmetlerimiz' }
];

const brandServices = [
  {
    title: "Hyundai Servis",
    desc: "i10, i20, Tucson ve tüm modeller için uzman bakım.",
    link: "/hyundai-ozel-servis",
    logo: "/images/logolar/hyundai.webp",
    image: "/images/hyundai-bayon-servis.jpg",
    color: "blue"
  },
  {
    title: "Kia Servis",
    desc: "Sportage, Picanto, Ceed ve tüm modeller.",
    link: "/kia-ozel-servis",
    logo: "/images/logolar/kia.webp",
    image: "/images/kia-stonic-servis.jpg",
    color: "red"
  },
  {
    title: "Toyota Servis",
    desc: "Corolla, Yaris, C-HR ve hibrit uzmanlığı.",
    link: "/toyota-ozel-servis",
    logo: "/images/logolar/toyota.webp",
    image: "/images/toyota-c-hr-ozel-servis.jpg",
    color: "red"
  },
  {
    title: "Nissan Servis",
    desc: "Qashqai, Juke ve CVT şanzıman servisi.",
    link: "/nissan-ozel-servis",
    logo: "/images/logolar/nissan.webp",
    image: "/images/nissan-micra-servis.jpg",
    color: "slate"
  }
];

const generalServices = [
  {
    title: 'Periyodik Bakım',
    desc: 'Aracınızın üretici standartlarında periyodik bakımı ve kontrolü.',
    image: '/images/hizmetlerimiz/bakim-onarim.avif',
    icon: 'M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z'
  },
  {
    title: 'Motor Yenileme',
    desc: 'Komple motor rektefiye ve performans yenileme işlemleri.',
    image: '/images/hizmetlerimiz/Motor Yenileme.avif',
    icon: 'M10.325 4.317c.426-1.756 2.924-1.756 3.35 0a1.724 1.724 0 002.573 1.066c1.543-.94 3.31.826 2.37 2.37a1.724 1.724 0 001.065 2.572c1.756.426 1.756 2.924 0 3.35a1.724 1.724 0 00-1.066 2.573c.94 1.543-.826 3.31-2.37 2.37a1.724 1.724 0 00-2.572 1.065c-.426 1.756-2.924 1.756-3.35 0a1.724 1.724 0 00-2.573-1.066c-1.543.94-3.31-.826-2.37-2.37a1.724 1.724 0 00-1.065-2.572c-1.756-.426-1.756-2.924 0-3.35a1.724 1.724 0 001.066-2.573c-.94-1.543.826-3.31 2.37-2.37.996.608 2.296.07 2.572-1.065z'
  },
  {
    title: 'Arıza Tespit',
    desc: 'Son teknoloji cihazlarla detaylı arıza analizi ve raporlama.',
    image: '/images/hizmetlerimiz/ariza-tespiti.avif',
    icon: 'M9.75 17L9 20l-1 1h8l-1-1-.75-3M3 13h18M5 17h14a2 2 0 002-2V5a2 2 0 00-2-2H5a2 2 0 00-2 2v10a2 2 0 002 2z'
  },
  {
    title: 'Yağ Değişimi',
    desc: 'Orijinal yağ ve filtrelerle motorunuzun ömrünü uzatın.',
    image: '/images/hizmetlerimiz/6782ce88347fbdb8fab18539_general-maintenance-featured-image.avif',
    icon: 'M19.428 15.428a2 2 0 00-1.022-.547l-2.387-.477a6 6 0 00-3.86.517l-.318.158a6 6 0 01-3.86.517L6.05 15.21a2 2 0 00-1.806.547M8 4h8l-1 1v5.172a2 2 0 00.586 1.414l5 5c1.26 1.26.367 3.414-1.415 3.414H4.828c-1.782 0-2.674-2.154-1.414-3.414l5-5A2 2 0 009 10.172V5L8 4z'
  },
  {
    title: 'Şanzıman Tamiri',
    desc: 'Otomatik, Manuel ve CVT şanzıman revizyonu ve bakımı.',
    image: '/images/hizmetlerimiz/sanziman-tamiri.avif',
    icon: 'M4 4v5h.582m15.356 2A8.001 8.001 0 004.582 9m0 0H9m11 11v-5h-.581m0 0a8.003 8.003 0 01-15.357-2m15.357 2H15'
  },
  {
    title: 'Mekanik Onarım',
    desc: 'Alt takım, fren sistemi, debriyaj ve şanzıman onarımları.',
    image: '/images/hizmetlerimiz/Üst kapak rektefiye.avif',
    icon: 'M10 14l2-2m0 0l2-2m-2 2l-2-2m2 2l2 2m7-2a9 9 0 11-18 0 9 9 0 0118 0z'
  },
  {
    title: 'Enjektör & Pompa',
    desc: 'Dizel ve benzinli araçlar için yakıt sistemi bakımı.',
    image: '/images/hizmetlerimiz/2-diesel-fuel-injection-services-2-l6ge7.avif',
    icon: 'M13 10V3L4 14h7v7l9-11h-7z'
  },
  {
    title: 'Triger Değişimi',
    desc: 'Zamanında triger değişimi ile motorunuzu koruma altına alın.',
    image: '/images/hizmetlerimiz/triger-degisimi-fiyatlari.avif',
    icon: 'M12 8v4l3 3m6-3a9 9 0 11-18 0 9 9 0 0118 0z'
  }
];
---

<Layout title={pageTitle} description={pageDescription}>
  <BreadcrumbSchema items={breadcrumbItems} />

  <!-- HERO SECTION (Industrial & Clean) -->
  <section class="relative h-[60vh] min-h-[500px] flex items-center justify-center overflow-hidden bg-slate-900">
    <div class="absolute inset-0 z-0">
        <img 
            src="https://images.unsplash.com/photo-1486006920555-c77dcf18193c?auto=format&fit=crop&q=80" 
            alt="Tetik Otomotiv Servis Alanı" 
            class="w-full h-full object-cover object-center opacity-50" 
        />
        <div class="absolute inset-0 bg-gradient-to-t from-slate-900 via-slate-900/40 to-transparent"></div>
    </div>
    
    <div class="container mx-auto px-4 relative z-10 text-center">
        <div class="inline-block mb-6 px-4 py-2 border border-white/20 bg-white/5 backdrop-blur-sm rounded-full" data-reveal="fade-down">
            <span class="text-white/90 text-sm font-bold tracking-widest uppercase font-sans">Garantili & Kurumsal Hizmet</span>
        </div>
        <h1 class="text-4xl md:text-6xl lg:text-7xl font-bold text-white mb-6 tracking-tight font-sans max-w-4xl mx-auto" data-reveal="fade-up">
            SERVİS STANDARTLARINI <br/>
            <span class="text-transparent bg-clip-text bg-gradient-to-r from-orange-400 to-orange-600">YENİDEN TANIMLIYORUZ</span>
        </h1>
        <p class="text-lg text-slate-300 mb-10 max-w-2xl mx-auto font-sans leading-relaxed" data-reveal="fade-up" data-delay="100">
            Tuzla ve Gebze bölgesinde, üretici standartlarında bakım ve onarım hizmeti. Aracınızın performansını zirveye taşıyın.
        </p>
    </div>
  </section>

  <!-- BRAND SERVICES (Bento Grid - Dark Theme) -->
  <section class="py-24 bg-slate-950 relative overflow-hidden">
    <!-- Background Pattern -->
    <div class="absolute inset-0 opacity-[0.02]">
      <div class="absolute inset-0" style="background-image: linear-gradient(rgba(255,255,255,0.1) 1px, transparent 1px), linear-gradient(90deg, rgba(255,255,255,0.1) 1px, transparent 1px); background-size: 60px 60px;"></div>
    </div>

    <div class="container mx-auto px-4 relative z-10">
        <div class="flex flex-col md:flex-row justify-between items-end mb-16 gap-6" data-reveal="fade-up">
            <div>
                <span class="text-orange-500 font-bold tracking-widest uppercase text-sm font-sans">Uzmanlık Alanlarımız</span>
                <h2 class="text-3xl md:text-5xl font-bold text-white mt-3 font-heading tracking-normal">Markaya Özel Çözümler</h2>
            </div>
            <p class="text-slate-400 max-w-md font-sans leading-relaxed">
                Her marka için özel eğitimli teknisyenler, orijinal yedek parça ve diyagnostik cihazlarla hizmet.
            </p>
        </div>

        <!-- BENTO GRID -->
        <div class="grid md:grid-cols-2 lg:grid-cols-3 gap-6">
            
            <!-- HYUNDAI (Featured - Large) -->
            <a href="/hyundai-ozel-servis" class="group relative lg:row-span-2 min-h-[400px] lg:min-h-[500px] rounded-3xl overflow-hidden border border-slate-800 hover:border-blue-500/50 transition-all duration-500" data-reveal="fade-up">
                <div class="absolute inset-0">
                    <img src="/images/hyundai-bayon-servis.jpg" alt="Hyundai Servis" class="w-full h-full object-cover transition-transform duration-700 group-hover:scale-105" />
                </div>
                <div class="absolute inset-0 bg-gradient-to-t from-slate-950 via-slate-950/70 to-slate-950/20"></div>
                
                <div class="relative z-10 h-full flex flex-col justify-between p-8 lg:p-10">
                    <!-- Logo with Glow -->
                    <div class="relative">
                        <div class="absolute -inset-2 bg-blue-500/20 rounded-2xl blur-xl opacity-0 group-hover:opacity-100 transition-opacity duration-500"></div>
                        <div class="relative w-20 h-20 bg-white/95 backdrop-blur rounded-2xl flex items-center justify-center p-3 shadow-2xl transform group-hover:scale-110 transition-transform duration-500">
                            <img src="/images/logolar/hyundai.webp" alt="Hyundai" class="w-full h-auto object-contain" />
                        </div>
                    </div>

                    <div>
                        <!-- Stats Row -->
                        <div class="flex gap-6 mb-6">
                            <div>
                                <div class="text-2xl font-bold text-white font-sans">500+</div>
                                <div class="text-xs text-slate-400 uppercase tracking-wider">Araç Servisi</div>
                            </div>
                            <div>
                                <div class="text-2xl font-bold text-white font-sans">17</div>
                                <div class="text-xs text-slate-400 uppercase tracking-wider">Yıl Uzmanlık</div>
                            </div>
                        </div>

                        <h3 class="text-3xl lg:text-4xl font-bold text-white mb-3 font-heading">Hyundai Özel Servis</h3>
                        <p class="text-slate-300 font-sans leading-relaxed mb-6 max-w-sm">
                            i10, i20, Tucson, Elantra ve tüm Hyundai modelleri için yetkili servis kalitesinde bakım ve onarım.
                        </p>
                        
                        <div class="inline-flex items-center gap-3 bg-blue-600 hover:bg-blue-500 text-white px-6 py-3 rounded-xl font-bold transition-colors">
                            <span>Servisi İncele</span>
                            <svg class="w-5 h-5 transform group-hover:translate-x-1 transition-transform" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 8l4 4m0 0l-4 4m4-4H3"></path></svg>
                        </div>
                    </div>
                </div>
            </a>

            <!-- KIA -->
            <a href="/kia-ozel-servis" class="group relative min-h-[240px] rounded-3xl overflow-hidden border border-slate-800 hover:border-red-500/50 transition-all duration-500" data-reveal="fade-up" data-delay="100">
                <div class="absolute inset-0">
                    <img src="/images/kia-stonic-servis.jpg" alt="Kia Servis" class="w-full h-full object-cover transition-transform duration-700 group-hover:scale-110" />
                </div>
                <div class="absolute inset-0 bg-gradient-to-r from-slate-950 via-slate-950/80 to-transparent"></div>
                
                <div class="relative z-10 h-full flex items-center p-8">
                    <div class="flex items-center gap-6">
                        <!-- Logo -->
                        <div class="relative flex-shrink-0">
                            <div class="absolute -inset-2 bg-red-500/20 rounded-xl blur-lg opacity-0 group-hover:opacity-100 transition-opacity duration-500"></div>
                            <div class="relative w-16 h-16 bg-white/95 backdrop-blur rounded-xl flex items-center justify-center p-2 shadow-xl group-hover:scale-110 transition-transform duration-500">
                                <img src="/images/logolar/kia.webp" alt="Kia" class="w-full h-auto object-contain" />
                            </div>
                        </div>
                        
                        <div>
                            <h3 class="text-2xl font-bold text-white mb-1 font-heading">Kia Özel Servis</h3>
                            <p class="text-slate-400 text-sm font-sans mb-3">Sportage, Picanto, Ceed, Stonic</p>
                            <div class="flex items-center gap-4 text-xs text-slate-500">
                                <span class="flex items-center gap-1">
                                    <span class="w-1.5 h-1.5 bg-green-500 rounded-full"></span>
                                    Orijinal Parça
                                </span>
                                <span>350+ Araç</span>
                            </div>
                        </div>
                    </div>
                    
                    <div class="ml-auto hidden md:block">
                        <div class="w-10 h-10 rounded-full bg-white/10 flex items-center justify-center group-hover:bg-red-500 transition-colors">
                            <svg class="w-5 h-5 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"></path></svg>
                        </div>
                    </div>
                </div>
            </a>

            <!-- TOYOTA -->
            <a href="/toyota-ozel-servis" class="group relative min-h-[240px] rounded-3xl overflow-hidden border border-slate-800 hover:border-red-500/50 transition-all duration-500" data-reveal="fade-up" data-delay="200">
                <div class="absolute inset-0">
                    <img src="/images/toyota-c-hr-ozel-servis.jpg" alt="Toyota Servis" class="w-full h-full object-cover transition-transform duration-700 group-hover:scale-110" />
                </div>
                <div class="absolute inset-0 bg-gradient-to-r from-slate-950 via-slate-950/80 to-transparent"></div>
                
                <div class="relative z-10 h-full flex items-center p-8">
                    <div class="flex items-center gap-6">
                        <!-- Logo -->
                        <div class="relative flex-shrink-0">
                            <div class="absolute -inset-2 bg-red-500/20 rounded-xl blur-lg opacity-0 group-hover:opacity-100 transition-opacity duration-500"></div>
                            <div class="relative w-16 h-16 bg-white/95 backdrop-blur rounded-xl flex items-center justify-center p-2 shadow-xl group-hover:scale-110 transition-transform duration-500">
                                <img src="/images/logolar/toyota.webp" alt="Toyota" class="w-full h-auto object-contain" />
                            </div>
                        </div>
                        
                        <div>
                            <h3 class="text-2xl font-bold text-white mb-1 font-heading">Toyota Özel Servis</h3>
                            <p class="text-slate-400 text-sm font-sans mb-3">Corolla, Yaris, C-HR, Hibrit</p>
                            <div class="flex items-center gap-4 text-xs text-slate-500">
                                <span class="flex items-center gap-1">
                                    <span class="w-1.5 h-1.5 bg-green-500 rounded-full"></span>
                                    Hibrit Uzmanı
                                </span>
                                <span>400+ Araç</span>
                            </div>
                        </div>
                    </div>
                    
                    <div class="ml-auto hidden md:block">
                        <div class="w-10 h-10 rounded-full bg-white/10 flex items-center justify-center group-hover:bg-red-500 transition-colors">
                            <svg class="w-5 h-5 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"></path></svg>
                        </div>
                    </div>
                </div>
            </a>

            <!-- NISSAN (Full Width Bottom) -->
            <a href="/nissan-ozel-servis" class="group relative lg:col-span-2 min-h-[200px] rounded-3xl overflow-hidden border border-slate-800 hover:border-slate-500/50 transition-all duration-500" data-reveal="fade-up" data-delay="300">
                <div class="absolute inset-0">
                    <img src="/images/nissan-micra-servis.jpg" alt="Nissan Servis" class="w-full h-full object-cover transition-transform duration-700 group-hover:scale-105" />
                </div>
                <div class="absolute inset-0 bg-gradient-to-r from-slate-950 via-slate-950/70 to-slate-950/50"></div>
                
                <div class="relative z-10 h-full flex items-center justify-between p-8">
                    <div class="flex items-center gap-6">
                        <!-- Logo -->
                        <div class="relative flex-shrink-0">
                            <div class="absolute -inset-2 bg-slate-400/20 rounded-xl blur-lg opacity-0 group-hover:opacity-100 transition-opacity duration-500"></div>
                            <div class="relative w-16 h-16 bg-white/95 backdrop-blur rounded-xl flex items-center justify-center p-2 shadow-xl group-hover:scale-110 transition-transform duration-500">
                                <img src="/images/logolar/nissan.webp" alt="Nissan" class="w-full h-auto object-contain" />
                            </div>
                        </div>
                        
                        <div>
                            <h3 class="text-2xl font-bold text-white mb-1 font-heading">Nissan Özel Servis</h3>
                            <p class="text-slate-400 text-sm font-sans mb-3">Qashqai, Juke, Micra, X-Trail</p>
                            <div class="flex items-center gap-4 text-xs text-slate-500">
                                <span class="flex items-center gap-1">
                                    <span class="w-1.5 h-1.5 bg-orange-500 rounded-full"></span>
                                    CVT Şanzıman Uzmanı
                                </span>
                                <span>300+ Araç</span>
                            </div>
                        </div>
                    </div>
                    
                    <div class="hidden md:flex items-center gap-6">
                        <!-- Mini Stats -->
                        <div class="text-right">
                            <div class="text-xl font-bold text-white font-sans">CVT</div>
                            <div class="text-xs text-slate-500 uppercase">Şanzıman Servisi</div>
                        </div>
                        <div class="w-12 h-12 rounded-full bg-white/10 flex items-center justify-center group-hover:bg-slate-600 transition-colors">
                            <svg class="w-6 h-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"></path></svg>
                        </div>
                    </div>
                </div>
            </a>

        </div>
    </div>
  </section>

  <!-- GENERAL SERVICES (Image + Gradient Cards) -->
  <section class="py-24 bg-slate-950 text-white relative overflow-hidden">
    <div class="absolute inset-0 bg-[radial-gradient(ellipse_at_top_right,_var(--tw-gradient-stops))] from-slate-800/50 via-slate-950 to-slate-950"></div>
    
    <div class="container mx-auto px-4 relative z-10">
        <div class="text-center mb-20" data-reveal="fade-up">
            <h2 class="text-3xl md:text-5xl font-bold text-white mb-6 font-sans">GENEL SERVİS HİZMETLERİ</h2>
            <p class="text-slate-400 max-w-2xl mx-auto font-sans text-lg">
                Aracınızın ihtiyacı olan tüm mekanik ve elektronik çözümler tek çatı altında.
            </p>
        </div>

        <div class="grid md:grid-cols-2 lg:grid-cols-4 gap-6">
            {generalServices.map((service, index) => (
                <div class="group relative h-[400px] rounded-2xl overflow-hidden border border-white/10 hover:border-orange-500/50 transition-all duration-500" data-reveal="fade-up" data-delay={index * 100}>
                    
                    <!-- Background Image -->
                    <div class="absolute inset-0">
                        <img 
                            src={service.image} 
                            alt={service.title} 
                            class="w-full h-full object-cover transition-transform duration-700 group-hover:scale-110"
                        />
                    </div>

                    <!-- Gradient Overlay (Primary) -->
                    <div class="absolute inset-0 bg-gradient-to-t from-slate-950 via-slate-950/80 to-transparent opacity-90 group-hover:opacity-80 transition-opacity duration-500"></div>
                    
                    <!-- Secondary Hover Gradient -->
                    <div class="absolute inset-0 bg-gradient-to-t from-orange-900/50 to-transparent opacity-0 group-hover:opacity-100 transition-opacity duration-500 mix-blend-overlay"></div>

                    <!-- Content -->
                    <div class="relative z-10 h-full flex flex-col justify-end p-8">
                        <div class="mb-auto transform translate-y-4 opacity-0 group-hover:translate-y-0 group-hover:opacity-100 transition-all duration-500 delay-100">
                             <div class="w-12 h-12 bg-orange-600/90 backdrop-blur rounded-xl flex items-center justify-center text-white shadow-lg">
                                <svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d={service.icon}></path>
                                </svg>
                            </div>
                        </div>

                        <h3 class="text-2xl font-bold text-white mb-3 font-sans transform group-hover:-translate-y-2 transition-transform duration-300">{service.title}</h3>
                        <p class="text-slate-300 font-sans text-sm leading-relaxed opacity-80 group-hover:opacity-100 transition-opacity duration-300">
                            {service.desc}
                        </p>
                        
                        <div class="mt-6 pt-6 border-t border-white/10 flex items-center text-orange-400 font-bold text-sm uppercase tracking-wider opacity-0 translate-y-4 group-hover:opacity-100 group-hover:translate-y-0 transition-all duration-500">
                            <span>İncele</span>
                            <svg class="w-4 h-4 ml-2" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 8l4 4m0 0l-4 4m4-4H3"></path></svg>
                        </div>
                    </div>
                </div>
            ))}
        </div>
    </div>
  </section>

  <!-- CTA / EMERGENCY -->
  <section class="py-20 bg-white">
    <div class="container mx-auto px-4">
        <div class="bg-orange-600 rounded-3xl p-12 md:p-20 text-center relative overflow-hidden shadow-2xl shadow-orange-900/20">
             <div class="absolute inset-0 bg-[url('/images/pattern.svg')] opacity-10 mix-blend-overlay"></div>
             <div class="absolute top-0 right-0 w-64 h-64 bg-white opacity-10 rounded-full blur-3xl -mr-32 -mt-32"></div>
             <div class="absolute bottom-0 left-0 w-64 h-64 bg-black opacity-10 rounded-full blur-3xl -ml-32 -mb-32"></div>
             
             <div class="relative z-10 max-w-3xl mx-auto">
                <h2 class="text-3xl md:text-5xl font-bold text-white mb-6 font-sans">Acil Durum mu?</h2>
                <p class="text-white/90 text-xl mb-10 font-sans">
                    Yolda kalmanıza gerek yok. 7/24 Acil Yol Yardım hattımızı arayın, en kısa sürede yanınızda olalım.
                </p>
